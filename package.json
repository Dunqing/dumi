{
  "name": "dumi",
  "version": "0.2.1",
  "private": "true",
  "description": "vite-plugin-dumi",
  "license": "MIT",
  "author": "DengQing <dengqing0821@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunqing/vite-plugin-dumi.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "pnpm run --filter ./packages --parallel dev",
    "build": "pnpm run --filter ./packages build",
    "play": "pnpm run --filter ./playground dev",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://github.com/Dunqing/vite-plugin-dumi#readme",
  "bugs": {
    "url": "https://github.com/Dunqing/vite-plugin-dumi/issues"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.6",
    "@types/mdast": "3.0.10",
    "@types/node": "^17.0.24",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "pnpm": "^6.32.8",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vitest": "^0.9.3"
  }
}
