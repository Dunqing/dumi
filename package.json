{
  "name": "vite-plugin-dumi",
  "version": "0.2.1",
  "description": "vite-plugin-vite-plugin-dumi",
  "license": "MIT",
  "author": "DengQing <dengqing0821@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunqing/vite-plugin-dumi.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "play": "pnpm run --filter ./playground dev",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://github.com/Dunqing/vite-plugin-dumi#readme",
  "bugs": {
    "url": "https://github.com/Dunqing/vite-plugin-dumi/issues"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "fast-glob": "^3.2.11",
    "rehype-parse": "^8.0.4",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "remark-yaml-config": "^6.0.1",
    "unified": "^10.1.2",
    "unist-util-is": "^5.1.1",
    "unist-util-visit": "^4.1.0",
    "vite": "^2.9.5",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.6",
    "@types/mdast": "3.0.10",
    "@types/node": "^17.0.24",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "pnpm": "^6.32.8",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vitest": "^0.9.3"
  }
}
